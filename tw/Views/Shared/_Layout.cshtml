<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <!-- Basic Page Needs -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Offshore - Responsive HTML Template">
    <meta name="author" content="Surjith S.M">
    <!-- Page Title -->
    <title> 天沃科技 - @ViewBag.Title</title>
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="favicon.png">
    <!-- Mobile Specific Metas -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap core CSS -->
    <link href="css/bootstrap.min.css" rel="stylesheet">
    <!-- Font Awesome CSS -->
    <link rel="stylesheet" href="fonts/font-awesome/css/font-awesome.min.css">
    <!-- Flex Slider -->
    <link rel="stylesheet" href="css/flexslider.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="css/owl.carousel.min.css">
    <link rel="stylesheet" href="css/owl.theme.min.css">
    <!-- Custom styles for this template -->
    <link href="css/style.css" rel="stylesheet">
</head>
<body class="homepage">
    <!-- Preloader -->
    <div class="loader-wrapper">
        <div class="sk-cube-grid">
            <div class="sk-cube sk-cube1"></div>
            <div class="sk-cube sk-cube2"></div>
            <div class="sk-cube sk-cube3"></div>
            <div class="sk-cube sk-cube4"></div>
            <div class="sk-cube sk-cube5"></div>
            <div class="sk-cube sk-cube6"></div>
            <div class="sk-cube sk-cube7"></div>
            <div class="sk-cube sk-cube8"></div>
            <div class="sk-cube sk-cube9"></div>
        </div>
    </div>
    <!-- Page Wrapper -->
    <div class="wrapper">
        <!-- Header Section -->
        <header>
            <div class="header-area">
                <!-- Top Contact Info -->
                <div class="row logo-top-info">
                    <div class="container">
                        <div class="col-md-3 logo">
                            <!-- Main Logo -->
                            <a href="index.html"><img src="images/tw_logo.jpg" alt="Offshore Logo" /></a>
                            <!-- Responsive Toggle Menu -->
                            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbar">
                                <span class="sr-only"> Main Menu </span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                        </div>
                        <div class="col-md-9 top-info-social">
                            <div class="pull-right">
                                <div class="top-info">
                                    <div class="call">
                                        <h3> 联系我们 </h3>
                                        <p> 1800 425 4646 </p>
                                    </div>
                                    <div class="email">
                                        <h3> 客服邮箱 </h3>
                                        <p> info@offshoreinsustry.com </p>
                                    </div>
             
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
                <!-- Main Navigation Section -->
                <nav id="navbar" class="collapse navbar-collapse main-menu">
                    <div class="container">
                        <ul class="main-menu">
                            <li class="active"> <a href="index"> 首页 </a></li>
                            <li class="dropdown">
                                <a href="#" data-toggle="dropdown">
                                    关于我们
                                    <i class="fa fa-chevron-down dropdown-toggle"> </i>
                                </a>
                                <ul>
                                    <li> <a href="about"> 走近天沃 </a> </li>
                                    <li> <a href="presence"> 天沃风采 </a> </li>
                                    <li> <a href="team"> 我们的团队 </a> </li>
                                    <li> <a href="contact">联系我们 </a> </li>
                                </ul>
                            </li>
                            <li class="dropdown">
                                <a href="#" data-toggle="dropdown">
                                    我们的服务
                                    <i class="fa fa-chevron-down dropdown-toggle"> </i>
                                </a>
                                <ul>
                                    <li> <a href="services.html"> 供应链质量管理 </a> </li>
                                    <li> <a href="services-list.html"> 实验室LIMS软件系统 </a> </li>
                                    <li> <a href="services-single.html"> 技术标准服务 </a> </li>
                                    <li> <a href="services-full-width.html"> 第三方数据分析服务 </a> </li>
                                </ul>
                            </li>
                            <li><a href="technology.html"> 解决方案 </a> </li>
                            <li><a href="blog.html"> 业界动态 </a> </li>
                            <li><a href="careers.html"> 加入我们 </a> </li>
                           
                        </ul>
                    </div>
                </nav>
            </div>
        </header>
        <!-- Header Section -->
        <!-- Main Content Section -->
        @RenderBody()
        <!-- Main Content Section -->
        <!-- Footer Section -->
        <footer>
            <div class="footer">
                <div class="container">
                  
                    <!-- Footer widgets -->
                    <div class="row widgets">
                        <div class="col-md-4 col-sm-6">
                            <div class="about-txt widget">
                                <div class="foot-logo">
                                    <img src="images/tw_logo.jpg" alt="logo" />
                                </div>
                                           
                            </div>
                        </div>
                        <div class="spacer-50 visible-sm"></div>
                        <div class="col-md-4 col-sm-7">
                            <div class="our-services widget">
                                <h4 class="widget-title">我们的服务</h4>
                                <ul>
                                    <li> <a href="services.html"> 供应链质量管理 </a> </li>
                                    <li> <a href="services-list.html"> 实验室LIMS软件系统 </a> </li>
                                    <li> <a href="services-single.html"> 技术标准服务 </a> </li>
                                    <li> <a href="services-full-width.html"> 第三方数据分析服务 </a> </li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-4 col-sm-7">
                            <div class="newsletter widget">
                                <div class="contact-details">
                                    <h4 class="pre-footer-title">联系我们</h4>
                                    <p>
                                        SUPPORT: 1800 425 4646
                                        <br> OFFICE: +1 (253) 2587 220
                                    </p>
                                </div>
                                <div class="contact-details">
                                    <h4 class="pre-footer-title">客服邮箱</h4>
                                    <p>
                                        hello@offshoreindustry.com
                                        <br> sales@offshoreindustry.com
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- Footer widgets -->
                </div>
            </div>
            <!-- Copyright -->
            <div class="copyright">
                <div class="container">
                    <div class="row copyright-bar">
                        <div class="col-md-6">
                            <p>Copyright © 2018  TianWo Supplychain Integration Service Co., Ltd  All rights reserved.</p>
                        </div>
                       
                    </div>
                </div>
            </div>
            <!-- Copyright -->
        </footer>
        <!-- Footer Section -->
        <!-- back-to-top link -->
        <a href="#0" class="cd-top"> Top </a>
    </div>
    <!-- Page Wrapper
    –––––––––––––––––––––––––––––––––––––––––––––––––– -->
    <!-- Javascripts
    –––––––––––––––––––––––––––––––––––––––––––––––––– -->
    <!-- Jquery Library -->
    <script src="js/jquery.min.js"></script>
    <!-- Bootstrap core JavaScript -->
    <script src="js/bootstrap.min.js"></script>
    <!-- jQuery Flex Slider -->
    <script src="js/jquery.flexslider-min.js"></script>
    <!-- Owl Carousel -->
    <script src="js/owl.carousel.min.js"></script>
    <!-- Counter JS -->
    <script src="js/waypoints.min.js"></script>
    <script src="js/jquery.counterup.min.js"></script>
    <!-- Back to top -->
    <script src="js/back-to-top.js"></script>
    <!-- Form Validation -->
    <script src="js/validate.js"></script>
    <!-- Subscribe Form JS -->
    <script src="js/subscribe.js"></script>
    <!-- Main JS -->
    <script src="js/main.js"></script>
</body>
</html>
